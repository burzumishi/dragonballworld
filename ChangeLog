	float DistancePointToLine(vec3_t v1, vec3_t v2, vec3_t point)
	{
       		 vec3_t vec, p2;

       		 AAS_ProjectPointOntoVector(point, v1, v2, p2);
       		 VectorSubtract(point, p2, vec);
       		 return VectorLength(vec);
	} //end of the function DistancePointToLine

